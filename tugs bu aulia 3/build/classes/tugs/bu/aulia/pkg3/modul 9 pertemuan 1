    public static void main(String[] args) {
        // TODO code application logic here
          String datasiswa[][]={
            {"Andini ","Makan"},
            {"Bella  ","Membaca"},
            {"Dea    ","Mendengarkan musik"},
        };
        System.out.println(datasiswa[1][1]);
        System.out.println();
        for(int j=0;j<datasiswa.length;j++){
            System.out.println("nama: "+datasiswa[j][0]+",Hobby:"+datasiswa[j][1]);
        }
    }
    
